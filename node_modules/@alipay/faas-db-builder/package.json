{
  "name": "@alipay/faas-db-builder",
  "version": "1.1.17",
  "description": "db builder",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "files": [
    "lib",
    "esm"
  ],
  "scripts": {
    "lint": "eslint src test --cache --fix --ext ts",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p ./tsconfig.json && minimize-js lib --minifyWhitespace --minifyIdentifiers",
    "build:esm": "tsc -p ./tsconfig.esm.json && minimize-js esm --minifyWhitespace --minifyIdentifiers",
    "dev": "tsc -p ./tsconfig.json",
    "clean": "tsc --build --clean",
    "test": "egg-bin test",
    "cov": "egg-bin cov",
    "ci": "npm run cov && npm run build",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@eggjs/tsconfig": "^1.3.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.1.3",
    "egg-bin": "^6.1.2",
    "eslint": "^8.34.0",
    "eslint-config-egg": "^12.1.0",
    "minimize-js": "^1.3.1",
    "mm": "^3.2.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
